{
    "name": "omnicode/lara-validation",
    "description": "Convenient wrapper for Laravel validation",
    "keywords": ["laravel", "validation", "validator"],
    "license": "MIT",
    "authors": [
        {
            "name": "OmniCode"
        }
    ],
    "require": {
        "laravel/framework": "5.8.*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.0",
        "omnicode/lara-test": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "LaraValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
